{
  "expo": {
    "name": "Spare",
    "slug": "spare",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "A beautifully simple family budgeting app",
    "icon": "./assets/images/icon.png",
    "scheme": "spare",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#111111",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "ios": {
      "bundleIdentifier": "get.spare.app",
      "supportsTablet": false,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "get.spare.app",
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#111111"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "98bb1b56-f159-4006-b765-d026e2411224"
      }
    },
    "owner": "samrithshankar"
  }
}
